{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "react-app"],
  "plugins": ["react", "import"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "indent": ["warn", 2],
    "no-prototype-builtins": "off"
  }
}